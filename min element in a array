import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner c=new Scanner(System.in);
        int i,min=999;
        int n;
        n=c.nextInt();
        int a[]=new int[n];
        for(i=0;i<n;i++)
        {
            a[i]=c.nextInt();
        }
        for(i=0;i<n;i++)
        {
            if(a[i]<min)
            {
                min=a[i];
            }
        }
        System.out.println(min);
    }
}
